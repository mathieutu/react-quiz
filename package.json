{
  "name": "react-qcm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prestart": "yarn build:css",
    "prebuild": "NODE_ENV=production yarn build:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint '**/*.{ts*, js}' --fix",
    "typecheck": "tsc --noEmit",
    "build:css": "postcss node_modules/tailwindcss/tailwind.css -o src/assets/css/tailwind.css",
    "generate:graphql": "graphql-codegen -r dotenv/config"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "date-fns": "^2.16.1",
    "graphql": "^15.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.0",
    "react-syntax-highlighter": "^15.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.7",
    "@tailwindcss/ui": "^0.6.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "autoprefixer": "^9.8.6",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "postcss-cli": "^8.1.0",
    "tailwindcss": "^1.9.6",
    "typescript": "~4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
